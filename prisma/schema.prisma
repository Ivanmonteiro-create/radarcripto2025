generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Trade {
  id        Int      @id @default(autoincrement())
  ts        DateTime @default(now())
  side      String
  symbol    String
  price     Float
  sizeUSDT  Float
  pnl       Float?
}
